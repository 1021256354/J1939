#include  "includes.h"

void HIB_InitHibernat(void);


osThreadDef (thread_CANComm, osPriorityHigh, 1, 1024); //periodï¼š100ms


int main(void)
{
	 volatile uint32_t *address;
      DAVE_Init();
      address = 0x08000000;
      *address=0X5;

      thread_CANComm_ID = osThreadCreate(osThread(thread_CANComm), NULL);

      osKernelStart();
}

